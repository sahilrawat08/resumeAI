{
  "name": "resumeai",
  "version": "1.0.0",
  "description": "AI-powered resume optimization web app",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test:all": "cd backend && npm test && cd ../frontend && npm test",
    "lint:all": "cd backend && npm run lint && cd ../frontend && npm run lint",
    "format:all": "cd backend && npm run format && cd ../frontend && npm run format"
  },
  "keywords": [
    "resume",
    "ai",
    "nlp",
    "ats",
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "openai",
    "typescript"
  ],
  "author": "Sahil Rawat",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sahilrawat08/resumeAI.git"
  },
  "bugs": {
    "url": "https://github.com/sahilrawat08/resumeAI/issues"
  },
  "homepage": "https://github.com/sahilrawat08/resumeAI#readme"
}